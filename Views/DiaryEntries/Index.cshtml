@model List<DiaryEntry>

<div class="table-responsive">
    <table class="table table-striped table-hover">
        <thead>
            <td>Title</td>
            <td>Content</td>
            <td>Date</td>
            <td>Actions</td>
        </thead>
        <tbody>
            @foreach (var entry in Model)
            {
                <tr>
                    <td>@entry.Title</td>
                    <td>@entry.Content</td>
                    <td>@entry.DateCreated</td>
                    <td>Edit and delete</td>
                </tr>
            }
        </tbody>
    </table>
</div>